    "A=9.5\n",
    "AA=7.5\n",
    "B=265.5\n",
    "BB=237.3\n",
    "T=input('TempC=')\n",
    "TD=input('DewC=')\n",
    "e=6.1078*10**((TD*AA)/(TD+BB))\n",
    "es=6.1078*10**((T*A)/(T+B))\n",
    "RHi=(e/es)*100\n",
    "S=RHi-100\n",
    "print e-es\n",
    "print'RHi: %r%s' % (RHi,'%')\n",
    "print'Supersaturation: %r%s' % (S,'%')"
   ]
  }
